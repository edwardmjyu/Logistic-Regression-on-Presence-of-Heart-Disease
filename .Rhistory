# Project: Data Wrangling Exercise 1: Basic Data Manipulation
#load libraries
library(dplyr)
library(tidyr)
library(utils)
#0 Load the data in RStudio
mydata <- read.csv("refine_original.csv")
mydata %>% glimpse()
#setting to local dataframe
refine <-tbl_df(mydata)
refine
#1 Clean up brand names
#Checking changes
refine %>%
group_by(company) %>%
tally()
#lowercase
refine <- refine %>%
mutate(company = tolower(refine$company))
#misspellings
refine <- refine %>%
mutate(company = gsub(pattern = "^.*z.*$", replacement = "akzo", refine$company))
refine <- refine %>%
mutate(company = gsub(pattern = "^.*ps$", replacement = "phillips", refine$company))
refine <- refine %>%
mutate(company = gsub(pattern = "^.*ver$", replacement = "unilever", refine$company))
#2 Separate product code and number
#use tidyr separate function
refine <- separate(data = refine, col = Product.code...number, into = c("product_code", "product_number"), sep = "-")
#3 Add product category
pcode <- rbind("p","v","x","q")
product_category <- rbind("Smartphone","TV","Laptop","Tablet")
pcodename<-data.frame(pcode, product_category, stringsAsFactors = FALSE)
refine <- left_join(refine, pcodename, by = c("product_code"="pcode"))
#4 Full address for geocoding
refine <- mutate(refine,
full_address = paste(address, city, country, sep = ','))
#5 Creating dummy variables
#company
for(level in unique(refine$company)){
refine[paste("company", level, sep = "_")] <- ifelse(refine$company == level, 1, 0)
}
#product
for(level in unique(refine$product_category)){
refine[paste("product", level, sep = "_")] <- ifelse(refine$product_category == level, 1, 0)
}
View(refine)
write.csv(refine, file = "refine_clean.csv")
